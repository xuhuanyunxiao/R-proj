z.test <- function(data,mu,thegma, alternative = 'twoside'){ # 数据、均值、标准差、单双侧
SE = thegma/sqrt(length(data))
Z = (mean(data) - mu)/SE
if (alternative == 'twoside') p = Z*(1-pnorm(abs(Z)))
else if (alternative == 'less') p = pnorm(Z)
else p = 1- pnorm(Z)
return(list(Z=Z,p=p))}
